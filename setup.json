{
    "name": "LunoxKit",
    "version": "1.1",
    "requirements": ["pyinstaller", "requests", "bs4", "colorama", "prettytable", "user_agent", "paramiko"],
    "timeDelay": 0.1,
    "cmd": {
        "pip": "/usr/bin/pip3",
        "py": "python3",
        "installer": "pyinstaller"
    },
    "args": {
        "pyinstaller": {
            "output": "build/lunox-{v}_build-{y}",
            "icon": ""
        },
        "pip": {
            "install": "install"
        }
    }
}